{
  "name": "bashtorio-core",
  "version": "0.1.0",
  "description": "Unix Pipe Factory Game - Core library",
  "type": "module",
  "main": "./dist/bashtorio-core.umd.cjs",
  "module": "./dist/bashtorio-core.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/bashtorio-core.js",
      "require": "./dist/bashtorio-core.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "imports": {
    "#v86": "./vendor/v86/libv86.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "lit-html": "^3.3.2"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.0"
  },
  "license": "MIT"
}
